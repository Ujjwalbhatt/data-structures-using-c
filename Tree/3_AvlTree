#include<stdio.h>
#include<stldlib.h>
 struct node{
    int key;
    struct node* left;
    struct node*right;
    int height;
 };
  
  int max(int a, int b)

  int height(struct node *n){
    if(n == NULL){
    return 0;
    }
    return n->height;
  }
  int max(int a, int b){
    return (a>b) 
  }

 